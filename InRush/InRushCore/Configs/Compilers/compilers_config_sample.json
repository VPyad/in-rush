{
  "ram_limit_process" : "100000", //in kb
  "compilers": [
    {
      "name": ".Net",
      "lang": "c#",
      "id": ".Net c#",
      "invocation": "dotnet",
      "version": "--version",
      "commands": [
        "#cd",
        "rm -r *",
        "new console",
        "#copy",
        "clean",
        "build",
        "run",
        "#read_output"
      ],
      "timeout": "10000"
    },
    {
      "name": "GCC",
      "lang": "c++",
      "id": "GCC c++",
      "invocation": "g++",
      "version": "--version",
      "commands": [
        "#cd",
        "rm -r *",
        "#copy",
        "program.cpp",
        "#execute [!Linux: ./a.out], [!Windows: ./a]", // limit RAM: (ulimit -v 150000; ./a.out) (linux)
        "#read_output"
      ],
      "timeout": "10000"
    },
